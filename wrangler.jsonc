  "compatibility_date": "2024-01-01",
  "compatibility_flags": ["nodejs_compat"],
  "kv_namespaces": [
    {
      "binding": "ACCOUNTS",
      "id": "placeholder_account_id"
    },
    {
      "binding": "MEMORIES",
      "id": "placeholder_memories_id"
    }
  ]
}
