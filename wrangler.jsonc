{
  // Cloudflare Workers configuration for MITOU Optimizer
  "name": "mitou",
  "main": "worker.ts",
  "compatibility_date": "2024-01-01",
  "compatibility_flags": ["nodejs_compat"],
  "kv_namespaces": [
    {
      "binding": "USERS_KV",
      "id": "placeholder_id_for_production",
      "preview_id": "placeholder_id_for_preview"
    },
    {
      "binding": "MEMORIES_KV",
      "id": "placeholder_id_for_production",
      "preview_id": "placeholder_id_for_preview"
    }
  ]
}
